                type="M2E\M2ECloudMagentoConnector\Model\Order\ShippingInformation"/>
    <preference for="M2E\M2ECloudMagentoConnector\Api\Data\TaxInterface"
                type="M2E\M2ECloudMagentoConnector\Model\Order\Tax"/>
    <preference for="M2E\M2ECloudMagentoConnector\Api\Data\TotalsInterface"
                type="M2E\M2ECloudMagentoConnector\Model\Order\Totals"/>

    <preference for="M2E\M2ECloudMagentoConnector\Api\OrderManagementInterface"
                type="M2E\M2ECloudMagentoConnector\Model\OrderManagement"/>

    <!-- Module list info -->
    <preference for="M2E\M2ECloudMagentoConnector\Api\ModuleInfoInterface"
                type="M2E\M2ECloudMagentoConnector\Model\ModuleInfoProvider"/>
</config>
